#-----------Find Command------------#
####### global select
funckey FAQ 'setwindow form.find;FORM find all_off;setwindow pcb' #clear all
funckey FAA 'setwindow form.find;FORM find all_on;setwindow pcb' #clear all
####### Only find the elements
funckey FC  'setwindow form.find;FORM find all_off;FORM find clines YES;FORM find cline_segs YES;setwindow pcb' #clines
funckey FB  'setwindow form.find;FORM find all_off;FORM find symbols YES;setwindow pcb' #choose symbol
funckey FD  'setwindow form.find;FORM find all_off;FORM find drc_errors YES;setwindow pcb' #DRC errors
funckey FE  'setwindow form.find;FORM find all_off;FORM find nets YES;setwindow pcb'  #choose net
funckey FF  'setwindow form.find;FORM find all_off;FORM find pins YES;setwindow pcb' #pin
funckey FG  'setwindow form.find;FORM find all_off;FORM find groups YES;setwindow pcb' #groups
funckey FS  'setwindow form.find;FORM find all_off;FORM find shapes YES;setwindow pcb' #shapes 
funckey FT  'setwindow form.find;FORM find all_off;FORM find text YES;setwindow pcb' #text
funckey FV  'setwindow form.find;FORM find all_off;FORM find vias YES;setwindow pcb' #via
funckey FW  'setwindow form.find;FORM find all_off;FORM find lines YES;FORM find other_segs YES;setwindow pcb' #lines
funckey FX  'setwindow form.find;FORM find all_off;FORM find voids YES;FORM find other_segs YES;setwindow pcb' #Void/Cavity  
#funckey FR #RATs
####### Add find the elements
funckey FAB 'setwindow form.find;FORM find symbols YES;setwindow pcb' #add choose symbol
funckey FAC 'setwindow form.find;FORM find clines YES;FORM find cline_segs YES;setwindow pcb' #add choose clines
funckey FAD 'setwindow form.find;FORM find drc_errors YES;setwindow pcb' #add choose DRC errors
funckey FAE 'setwindow form.find;FORM find nets YES;setwindow pcb'  #add choose net
funckey FAF 'setwindow form.find;FORM find pins YES;setwindow pcb' #add choose pin
funckey FAG 'setwindow form.find;FORM find groups YES;setwindow pcb' #groups
funckey FAS 'setwindow form.find;FORM find shapes YES;setwindow pcb' #add choose shapes 
funckey FAT 'setwindow form.find;FORM find text YES;setwindow pcb' #add choose texy
funckey FAV 'setwindow form.find;FORM find vias YES;setwindow pcb' #add choose via
funckey FAW 'setwindow form.find;FORM find lines YES;FORM find other_segs YES;setwindow pcb' #add choose lines 
funckey FAX  'setwindow form.find;FORM find all_off;FORM find voids YES;FORM find other_segs YES;setwindow pcb' #Void/Cavity 

