funckey 2CS "prepopup;pop dyn_option_select 'Snap pick to@:@Symbol Origin'" # 2CS for snap to symbol origin

funckey 2V "prepopup;pop dyn_option_select 'Snap pick to@:@Via'" # 2V for snap to via
funckey 2A "prepopup;pop dyn_option_select 'Snap pick to@:@Pin'" # 2A for snap to pin
funckey 2F "prepopup;pop dyn_option_select 'Snap pick to@:@Figure'" # 2F for snap to figure

