#-----------Ctrl Command-------------#
funckey ~A save_as
####### ~B to switch between bubble modes during routing
funckey ~B 'settoggle hug "Off" "Hug only" "Hug preferred" "Shove preferred" ; options bubble_space $hug'
#funckey ~E open
funckey ~F "prepopup ; pop dyn_option_select 'Selection set@:@Clear all selections';set prompt;prompt 'Find Ref Des';refdes $prompt;zoom selection"
#funckey ~G 
funckey ~Q 'settoggle routespace mindrc current user ; pop routespace $routespace; echo routespace = $routespace'
####### ~R for rotate in 90 degree increments
funckey ~R iangle 90
funckey ~S save
####### ~T toggles the corner degree during route
funckey ~T 'settoggle direction 90 45 Off; options lock_direction $direction; echo "Toggle Direction"'
####### ~V Search use net name
funckey ~V "prepopup ; pop dyn_option_select 'Selection set@:@Clear all selections';set prompt;prompt 'Find Net Name';net $prompt;zoom selection"
####### ~W to toggle cline (track) width during routing
funckey ~W 'settoggle minw .1 .3 .5 1 1.5 2; setwindow form.mini; FORM mini acon_line_width $minw; setwindow pcb'
funckey ~X redo
funckey ~Z undo

