#-----------Find Command------------#
####### global select
funckey FAQ 'setwindow form.find;FORM find all_off;setwindow pcb' #clear all
funckey FAA 'setwindow form.find;FORM find all_on;setwindow pcb' #clear all
####### Only find the elements
funckey FC  'setwindow form.find;FORM find all_off;FORM find clines YES;FORM find cline_segs YES;setwindow pcb' #clines
funckey FB  'setwindow form.find;FORM find all_off;FORM find symbols YES;setwindow pcb' #choose symbol
funckey FD  'setwindow form.find;FORM find all_off;FORM find drc_errors YES;setwindow pcb' #DRC errors
funckey FE  'setwindow form.find;FORM find all_off;FORM find nets YES;setwindow pcb'  #choose net
funckey FF  'setwindow form.find;FORM find all_off;FORM find pins YES;setwindow pcb' #pin
funckey FG  'setwindow form.find;FORM find all_off;FORM find groups YES;setwindow pcb' #groups
funckey FS  'setwindow form.find;FORM find all_off;FORM find shapes YES;setwindow pcb' #shapes 
funckey FT  'setwindow form.find;FORM find all_off;FORM find text YES;setwindow pcb' #text
funckey FV  'setwindow form.find;FORM find all_off;FORM find vias YES;setwindow pcb' #via
funckey FW  'setwindow form.find;FORM find all_off;FORM find lines YES;FORM find other_segs YES;setwindow pcb' #lines 
